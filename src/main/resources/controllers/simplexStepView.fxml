<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane layoutY="-1.0">
         <content>
            <VBox alignment="CENTER" prefHeight="800.0" prefWidth="1001.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="981.0">
                     <children>
                        <HBox prefHeight="39.0" prefWidth="407.0">
                           <children>
                              <Button fx:id="inFirstPage" mnemonicParsing="false" text="На главную" />
                           </children>
                        </HBox>
                        <Label fx:id="title" prefHeight="18.0" prefWidth="345.0" text="Label" />
                        <HBox alignment="TOP_RIGHT" prefHeight="39.0" prefWidth="419.0" spacing="10.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#changeViewFraction" text="переключить вид" />
                              <Button mnemonicParsing="false" onAction="#getSpravka" prefHeight="26.0" prefWidth="34.0" text="?" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <Pane fx:id="forTable" prefHeight="612.0" prefWidth="981.0" />
                  <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="back" mnemonicParsing="false" text="Назад" />
                        <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="900.0">
                           <children>
                              <Button fx:id="quickCount" mnemonicParsing="false" text="Быстро Досчитать" />
                              <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="331.0">
                                 <children>
                                    <Button fx:id="nextStep" mnemonicParsing="false" text="Следующий шаг" />
                                 </children>
                              </HBox>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
