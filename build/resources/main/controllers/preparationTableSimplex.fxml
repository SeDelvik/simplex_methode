<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox alignment="TOP_CENTER" prefHeight="688.0" prefWidth="1062.0">
         <children>
            <HBox>
               <children>
                  <HBox prefHeight="18.0" prefWidth="348.0" spacing="30.0">
                     <children>
                        <Button fx:id="save" mnemonicParsing="false" text="Сохранить" />
                        <Button fx:id="load" mnemonicParsing="false" text="Загрузить" />
                     </children>
                  </HBox>
                  <Label prefHeight="18.0" prefWidth="232.0" text="Введите целевую функцию и таблицу" />
                  <HBox alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="326.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#getSpravka" prefHeight="26.0" prefWidth="37.0" text="?" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox fx:id="forCellFunk" prefHeight="100.0" prefWidth="200.0" />
            <Pane fx:id="forGridPane" prefHeight="531.0" prefWidth="1042.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox prefHeight="100.0" prefWidth="502.0">
                     <children>
                        <Button fx:id="back" mnemonicParsing="false" text="Назад" />
                     </children>
                  </VBox>
                  <HBox prefHeight="100.0" prefWidth="433.0" spacing="50.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#createGraphicalMethode" text="графический метод" />
                        <Button fx:id="standartSimplex" mnemonicParsing="false" text="обычный базис" />
                        <Button fx:id="artificalSimplex" mnemonicParsing="false" text="Искусственный базис" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
